# {{ iptables_policy_name }} rules
# Parsing list 'iptables_allowed_from'
{% for ip_src_addr in iptables_allowed_from %}
-A INPUT -m state --state NEW -m {{ iptables_protocol|default('tcp') }} -p {{ iptables_protocol|default('tcp') }} --dport {{ iptables_port }} --source {{ ip_src_addr|default('0.0.0.0/0') }} -j ACCEPT
{% endfor %}
# End of rules for {{ iptables_policy_name }} role
